# lombok.config for JudahZone

# Ensure Lombok generates code for static @Getter/@Setter
lombok.fieldDefaults.defaultPrivate = false
lombok.fieldDefaults.defaultFinal = false

# Add generated annotation to help tools understand generated code
lombok.addLombokGeneratedAnnotation = true

# Generate suppressWarnings for generated code
lombok.addSuppressWarnings = false

# Improve compatibility with annotation processors
config.stopBubbling = true

# REMOVE OR COMMENT OUT THIS LINE:
# lombok.addNullAnnotations = javax

# Log configuration (helpful for debugging)
lombok.log.fieldName = log
lombok.log.fieldIsStatic = true

# Ensure static @Getter methods are properly generated
lombok.accessors.chain = false
lombok.accessors.fluent = false

# Clear generated directories on clean
lombok.extern.findbugs.addSuppressFBWarnings = false